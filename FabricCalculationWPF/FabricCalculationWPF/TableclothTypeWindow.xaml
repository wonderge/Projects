<Window x:Class="FabricCalculationWPF.TableclothTypeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FabricCalculationWPF"
        mc:Ignorable="d"
        Title="TableclothTypeWindow" Height="450" Width="500"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize">
    <StackPanel>
        <Button x:Name="btnRoundcloth" Content="圆形桌布" Click="btnRoundcloth_Click"/>
        <Button x:Name="btnTablecloth" Content="方形卓布" Margin="0, 200, 0, 0" Click="btnTablecloth_Click"/>
    </StackPanel>
</Window>
